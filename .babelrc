{
  "env": {
    "test": {
      "presets": [    [
        "@babel/preset-env", {
          "modules": "commonjs"
        }
      ]
      ]
    }
  },
  "presets": [
    [
      "@babel/preset-env", {
      "modules": true,
      "loose": true,
      "useBuiltIns": "usage"
    }
    ],
    [
      "@babel/preset-stage-2", {
      "modules": true,
      "loose": true,
      "useBuiltIns": true,
      "decoratorsLegacy": true
    }
    ]
  ],
  "plugins": [
    [
      "@babel/transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }
    ]
  ],
  "comments": false
}